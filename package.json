{
  "name": "@arconnect/warp-dre",
  "version": "0.0.1",
  "description": "Typescript module for Warp D.R.E. interactions",
  "repository": "https://github.com/arconnectio/warp-dre",
  "author": "Marton Lederer <marton@lederer.hu>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "fmt": "prettier . --write"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.5.9",
    "arweave": "^1.14.4",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3"
  }
}
